{
  "name": "mitra-web",
  "version": "3.23.0",
  "description": "Mitra web UI",
  "license": "AGPL-3.0-only",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "dist": "./scripts/dist.js",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test": "npm run test:unit",
    "lint:js": "eslint --ignore-pattern=dist/ .",
    "lint:css": "stylelint 'src/**/*.{vue,scss}'",
    "lint": "npm run typecheck && npm run lint:js && npm run lint:css",
    "start": "npm run serve",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "base64-js": "^1.5.1",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^6.13.5",
    "gemoji": "^8.1.0",
    "luxon": "^1.26.0",
    "pako": "^2.1.0",
    "qrcode": "^1.5.1",
    "viem": "^2.22.10",
    "vue": "~3.4.38",
    "vue-i18n": "^9.13.1",
    "vue-router": "~4.5.0",
    "vue3-click-away": "^1.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@lavamoat/allow-scripts": "^2.5.1",
    "@lavamoat/preinstall-always-fail": "^1.0.0",
    "@types/chai": "^4.2.11",
    "@types/luxon": "^1.26.5",
    "@types/mocha": "^5.2.4",
    "@types/pako": "^2.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/sinon": "^10.0.4",
    "@types/sinon-chai": "^3.2.5",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "~2.4.0",
    "chai": "^4.1.2",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.27.0",
    "globals": "^15.15.0",
    "jsdom": "^23.2.0",
    "postcss-html": "^1.7.0",
    "sass": "~1.69.7",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "stylelint": "^15.0.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.4.0",
    "vite": "^5.4.3",
    "vitest": "^1.6.0",
    "vue-tsc": "~2.0.28"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "vite>esbuild": false
    }
  },
  "overrides": {
    "esbuild": "0.25.0",
    "viem": {
      "ox": "0.6.7"
    }
  }
}
