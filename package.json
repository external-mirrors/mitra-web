{
  "name": "mitra-web",
  "version": "1.34.0",
  "description": "Mitra web UI",
  "license": "AGPL-3.0-only",
  "private": true,
  "engines": {
    "node": ">=12.2.0 || <16.0.0"
  },
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "dist": "./scripts/dist.js",
    "preview": "vite preview",
    "test:unit": "vite-jest",
    "test": "npm run test:unit",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
