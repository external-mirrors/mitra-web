{
  "name": "mitra-web",
  "version": "2.7.0",
  "description": "Mitra web UI",
  "license": "AGPL-3.0-only",
  "private": true,
  "engines": {
    "node": ">=14.18.0 || <20.0.0"
  },
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "dist": "./scripts/dist.js",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test": "npm run test:unit",
    "lint:js": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint 'src/**/*.{vue,scss}'",
    "lint": "npm run typecheck && npm run lint:js && npm run lint:css",
    "start": "npm run serve",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "base64-js": "^1.5.1",
    "bs58": "^5.0.0",
    "canonicalize": "^2.0.0",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.5.4",
    "luxon": "^1.26.0",
    "pako": "^2.1.0",
    "qrcode": "^1.5.1",
    "vue": "~3.2.44",
    "vue-router": "^4.0.15",
    "vue3-click-away": "^1.2.1"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^2.5.1",
    "@lavamoat/preinstall-always-fail": "^1.0.0",
    "@types/chai": "^4.2.11",
    "@types/luxon": "^1.26.5",
    "@types/mocha": "^5.2.4",
    "@types/pako": "^2.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/sinon": "^10.0.4",
    "@types/sinon-chai": "^3.2.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.1.0",
    "chai": "^4.1.2",
    "eslint": "^8.52.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "postcss-html": "^1.5.0",
    "sass": "~1.49.10",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "stylelint": "^14.16.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "typescript": "~4.5.5",
    "vite": "^3.2.6",
    "vite-plugin-html": "~2.0.6",
    "vitest": "^0.34.6",
    "vue-tsc": "~1.2.1"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "vite>esbuild": false
    }
  }
}
